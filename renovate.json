{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "pinDigests": true,
  "regexManagers": [
    {
      "fileMatch": [
        "^Dockerfile$"
      ],
      "matchStrings": [
        "ENV MSORT_VERSION=(?<currentValue>.*?)\n"
      ],
      "depNameTemplate": "utopia-planitia/msort",
      "datasourceTemplate": "github-tags"
    }
  ],
  "prHourlyLimit": 0,
  "labels": [
    "renovate-bot"
  ],
  "automerge": true,
  "automergeType": "pr",
  "prConcurrentLimit": 3,
  "dependencyDashboard": true,
  "dependencyDashboardAutoclose": true
}